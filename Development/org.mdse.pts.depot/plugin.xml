<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated Depot -->
      <package
            uri="http://mdse.org/pts/depot"
            class="org.mdse.pts.depot.DepotPackage"
            genModel="model/Depot.genmodel"/>
   </extension>
   
   <!-- extend to include validator on startup: -->
   <extension point="org.eclipse.ui.startup">
      <startup class="org.mdse.pts.depot.DepotValidator"/>
   </extension>

</plugin>
